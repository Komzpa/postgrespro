 {zitten,zit}
 {dragen,draag}
 {mooi}
 {boek,boeg}
 {ouder,oud}
 {deur}
 {betalen,betaal}
 {twintig}
(9 rows)

CREATE INDEX name_idx ON table1 USING GIN (to_tsvector('public.dutch', "name"));
